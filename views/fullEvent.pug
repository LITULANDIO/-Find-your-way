extends layout

block content
  .welcome.text-center.container
    p.welcome-top
      | #{retiros.title}
    .hr-line
  .my-container
    .row
      .col-md-7
        #article-photo-carousel.img-responsive.carousel.slide.article-slide
          .img-responsive.carousel-inner.cont-slider
            .item.active
              img.img-responsive(alt='', title='', src='https://www.amigosvalencia.com/imagenesblog/368296592.jpg')
            .item
              img.img-responsive(alt='', title='', src='http://placehold.it/600x400')
            .item
              img.img-responsive(alt='', title='', src='http://placehold.it/600x400')
            .item
              img.img-responsive(alt='', title='', src='http://placehold.it/600x400')
          ol.carousel-indicators
            li.active(data-slide-to='0', data-target='#article-photo-carousel')
              img(alt='', src='')
            li(data-slide-to='1', data-target='#article-photo-carousel')
              img(alt='', src='')
            li(data-slide-to='2', data-target='#article-photo-carousel')
              img(alt='', src='http://placehold.it/250x180')
            li(data-slide-to='3', data-target='#article-photo-carousel')
              img(alt='', src='http://placehold.it/250x180')
      .col-md-5
        .panel.panel-yellow
          .panel-heading.text-center Detalles
          .panel-body
            img.icon.icons8-Marker(src='data:image/png;base64,', width='26', height='26')
            |  Localidad: #{retiros.localitation}
            hr
            |  
            img(src='https://maxcdn.icons8.com/Android/PNG/24/Time_and_Date/calendar-24.png', title='Calendar', width='24')
            |  Fecha Inicio: #{retiros.startDate}
            br
            | &nbsp; &nbsp; &nbsp; &nbsp; Fecha Fin: #{retiros.endDate}
            hr
            |  
            img.icon.icons8-Clock(src='data:image/png;base64,', width='24', height='24')
            |  Hora Inicio: #{retiros.timeHourStart}
            br
            | &nbsp; &nbsp; &nbsp; &nbsp;&nbsp;Hora Fin: #{retiros.timeHourEnd}
            hr
            |  
            img.icon.icons8-Euro-Filled(src='data:image/png;base64,', width='26', height='26')
            |  Precio:
            br
  .row
    .col-md-7.panel-two
      .panel-two
        .panel.panel-yellow
          .panel-heading.text-center Descripci&oacute;n
          .panel-body
            |#{retiros.description}
    .col-md-5.panel-three
      .panel-two
        .panel.panel-yellow
          .panel-heading.text-center Compartir
          .panel-body.my-panel
            span.text-center.c
            a.social-icon.facebook(target='blank', data-tooltip='Facebook', href='http://www.facebook.com/SOJITRAHIREN')
              i.fa.fa-facebook
            a.social-icon.twitter(target='blank', data-tooltip='Twitter', href='https://www.twitter.com/Sojitra_Hiren')
              i.fa.fa-twitter
            a.social-icon.linkedin(target='blank', data-tooltip='LinkedIn', href='https://www.linkedin.com/in/hirensojitraamreli')
              i.fa.fa-linkedin
            a.social-icon.google-plus(target='blank', data-tooltip='Google +', href='https://plus.google.com/+HirenSojitraa')
              i.fa.fa-google-plus
            a.social-icon.email(target='blank', data-tooltip='Contact e-Mail', href='mailto:hirensojitra007@gmail.com')
              i.fa.fa-envelope-o
    .col-md-5.panel-four
      .panel-two
        .panel.panel-yellow
          .panel-heading.text-center Ubicaci√≥n
          .panel-body.my-panel
            span.text-center.c
           
    .col-md-5.panel-five
